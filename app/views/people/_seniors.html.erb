<li style="background-color: rgb(55,155,55,.4); ">
    <% @seniorId = seniors.people_id %>
    <% @tempPerson = Person.find_by(id: @seniorId) %>
    
    <% if @tempPerson %>
        <% if !@tempPerson.admin?%>
             <%= link_to @tempPerson.name, person_path(@tempPerson.id) %>
        <% end %>
           
        <% if Person.find_by(id: session[:tutor_id]).admin? && !@tempPerson.admin?%> 
            | <%= link_to "delete", Person.find_by(id: @seniorId), method: :delete,
                                          data: { confirm: "Are you sure you want to delete this account?" } %>
        <% end %>
    <% end %>
</li>